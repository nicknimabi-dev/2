<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!--mapper跟配置  namespace：需要映射dao的路径-->
<mapper namespace="com.qf.dao.CommonDao">
    <!--动态sql id 要执行的方法-->
    <select id="getOptions" resultType="String">
        select distinct ${column} from ${table}
    </select>

</mapper>
